{
	"name": "doxxo",
	"version": "0.2.0",
	"description": "A JavaScript documentation generator inspired by Docco.",
	"author": "Beneath the Ink <info@beneaththeink.com>",
	"contributors": [
		{
			"name": "Tyler Johnson",
			"email": "tyler@beneaththeink.com",
			"url": "http://github.com/tyler-johnson"
		}
	],
	"publishConfig": {
		"registry": "http://registry.npmjs.org"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/BeneathTheInk/doxxo.git"
	},
	"scripts": {
		"build-docs": "node ./bin/doxxo -i README.md README.md lib/"
	},
	"bin": {
		"doxxo": "./bin/doxxo"
	},
	"main": "./lib/doxxo.js",
	"dependencies": {
		"bluebird": "~2.7.1",
		"commondir": "0.0.1",
		"cpr": "~0.3.2",
		"dox": "~0.6.1",
		"highlight.js": "~8.4.0",
		"marked": "~0.3.2",
		"minimist": "~1.1.0",
		"mkdirp": "~0.5.0",
		"underscore": "~1.7.0"
	},
	"license": "MIT",
	"keywords": [
		"documentation",
		"docco",
		"comments",
		"block",
		"markdown",
		"inline"
	]
}
